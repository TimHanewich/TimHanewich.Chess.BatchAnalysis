<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Tim Hanewich</Company>
    <Authors>Tim Hanewich</Authors>
    <Version>1</Version>
    <Description>Class library for handling and analyzing PGN files at scale</Description>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <FileVersion>0.0.0.0</FileVersion>
    <PackageIcon>KnightIcon.jpg</PackageIcon>
    <PackageTags>chess,pgn,batch,analysis,file</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TimHanewich.Chess.Pgn" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="KnightIcon.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
